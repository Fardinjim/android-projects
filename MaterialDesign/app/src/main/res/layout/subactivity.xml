        <!-- This frame is the basic layout like linear layout
            in which you add ur content of the normal content-->
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="YOUR ARE IN SATHISH ACTIVITY"
                android:textSize="40dp"
                />
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="YOUR ARE IN LATHA ACTIVITY"
                android:textSize="40dp"
                 />
                <com.andexert.library.RippleView
                    android:id="@+id/more"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:rv_alpha="90"
                    app:rv_framerate="10"
                    app:rv_rippleDuration="400"
                    app:rv_color="#f00"
                    app:rv_centered="true"
                    app:rv_type="doubleRipple"

                    android:layout_margin="5dp"
                    rv_centered="true">
                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="click"/>
                </com.andexert.library.RippleView>
            </LinearLayout>
        </FrameLayout>

